<%- include('partials/header') %>
<h2>Search</h2>

<h3>Exact title</h3>
<form method="get" action="/books/searchexact">
  <label for="name">Title:</label>
  <input id="name" name="name" required>
  <button type="submit">Search (exact)</button>
</form>

<h3>Advanced (partial match)</h3>
<form method="get" action="/books/searchresult">
  <label for="keyword">Keyword:</label>
  <input id="keyword" name="keyword" required>
  <button type="submit">Search (advanced)</button>
</form>
<%- include('partials/footer') %>
