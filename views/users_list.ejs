<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title>Registered users</title>
    <link rel="stylesheet" href="/style.css" />
  </head>
  <body>
    <h1>Registered users</h1>

    <table>
      <tr>
        <th>Username</th>
        <th>First name</th>
        <th>Last name</th>
        <th>Email</th>
      </tr>
      <% users.forEach(function(user){ %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.first %></td>
        <td><%= user.last %></td>
        <td><%= user.email %></td>
      </tr>
      <% }) %>
    </table>

    <p><a href="/">Back to home</a></p>
  </body>
</html>
